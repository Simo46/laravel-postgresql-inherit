    "license": "MIT",
    "keywords": ["laravel", "postgres", "postresql", "database", "migrations", "inheritance"],
    "authors": [
        {
            "name": "Thibaud Dauce",
            "email": "thibaud@dauce.fr"
        },
        {
            "name": "Pasechnik Vlad",
